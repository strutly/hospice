/* components/pics/pics.wxss */
/*! CSS Used from: Embedded */
.weui_icon_cancel{position:absolute;top:0;right:0;transform:translate(50%,-50%);}
.weui_icon_cancel:before{content:"\EA0D";}
.weui_icon_cancel:before{color:#f43530;font-size:50rpx;}
.weui_uploader_file{margin-right:9px;margin-bottom:9px;background:no-repeat 50%;background-size:cover;}
.weui_uploader_input_wrp{    /*float: left;*/position:relative;margin-right:9px;margin-bottom:9px;width:79px;height:79px;border:1px solid #d9d9d9;}
.weui_uploader_input_wrp:after,.weui_uploader_input_wrp:before{content:" ";position:absolute;top:50%;left:50%;-webkit-transform:translate(-50%,-50%);transform:translate(-50%,-50%);background-color:#d9d9d9;}
.weui_uploader_input_wrp:before{width:2px;height:39.5px;}
.weui_uploader_input_wrp:after{width:39.5px;height:2px;}
.weui_uploader_input_wrp:active{border-color:#999;}
.weui_uploader_input_wrp:active:after,.weui_uploader_input_wrp:active:before{background-color:#999;}
.weui_uploader_input{position:absolute;z-index:1;top:0;left:0;width:100%;height:100%;opacity:0;-webkit-tap-highlight-color:rgba(0,0,0,0);}
.uploadImgs{/*float: left;*/margin-right:30rpx;margin-bottom:30rpx;max-width:28%;height:79px;display:inline-block;}
.uploadImgs image{margin:0;width: 79px;
  height: 79px;/*max-width:100%;max-height:100%;*/}
/* .weui_uploader_file{width:unset;} */
.img-upload{display:none;}
/*! CSS Used fontfaces */
@font-face{
  font-weight:400;
  font-style:normal;
  font-family:weui;
  src:url(data:application/octet-stream;base64,) format('truetype');
}
.weui_icon_cancel:before {
  font-family: weui;
  font-style: normal;
  font-weight: 400;
  display: inline-block;
  vertical-align: middle;
  text-decoration: inherit;
  width: 1em;
  margin-right: .2em;
  text-align: center;
  font-variant: normal;
  text-transform: none;
  line-height: 1em;
  margin-left: .2em;
}
.uploadImgs .jindu {
  z-index: 1;
  color: white;
  width: 100%;
  height: 100%;
  position: absolute;
  display: flex;
  align-items: center;
  justify-content: center;
  /* line-height: 79px; */
  text-align: center;
  background-color: rgba(0,0,0,.3);
}

.layui-input-block{position: relative;}
.img-uploading{
  position: absolute;
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #fff;
  /* background-color: rgba(0,0,0,.3); */
  z-index: 100;
}
.img-uploading::before{
  /* content:"上传中"; */
}
.hide{
  display:none !important;
}